		<script type="text/javascript">

		var Calendar = new Array();
		// Constant
		Calendar.CSS_ACTIVE = 'active',
		Calendar.CSS_INACTIVE = 'inactive',

		Calendar.MONTHS = new Array(
			'{{ Lang.Get('January') }}',
			'{{ Lang.Get('February') }}',
			'{{ Lang.Get('March') }}',
			'{{ Lang.Get('April') }}',
			'{{ Lang.Get('May') }}',
			'{{ Lang.Get('June') }}',
			'{{ Lang.Get('July') }}',
			'{{ Lang.Get('August') }}',
			'{{ Lang.Get('September') }}',
			'{{ Lang.Get('October') }}',
			'{{ Lang.Get('November') }}',
			'{{ Lang.Get('December') }}'
		),
		Calendar.Reload = function(year){
			var d = new Date();
			var currentYear = d.getFullYear();

			if(year > currentYear){
				return;
			}

			if($('.year-'+year).length){
				$('.year').hide();
				$('.year-'+year).show();
				$('.control-left, .control-right').data('year', year);
			}
		}

		// On document load
		$(document).ready(function(){
			// -----------------------------------------------------------------
			//  Calendar
			// -----------------------------------------------------------------
			// Init calendar
			var d = new Date(),
				year = d.getFullYear(),
				currentYear = d.getFullYear(),
				nbMonths, year, first, last, ctn;

			{% if Year %}
				year = {{ Year }};
			{% endif %}

			Calendar.Reload(year);

			{% if not isArchive %}
				$('.date li a').last().parent().addClass(Calendar.CSS_ACTIVE);
			{% endif %}

			year = $('.year').first();
			first = year.find('li:first-child a').text();
			ctn = $('<div></div>');

			// add month at start of first year if needed
			if(first !== Calendar.MONTHS[0]){
				$.each(Calendar.MONTHS, function(){
					if(this == first){
						year.prepend(ctn.html());
						return false;
					}
					ctn.append($('<li>'+this+'</li>'));
				});
			}

			// add month at end of last year if needed
			year = $('.year').last(),
			nbMonths = year.find('li').length;

			if (nbMonths < 12){
				for (i = nbMonths; i<12; i++){
					var e = $('<li>'+Calendar.MONTHS[i]+'</li>');
					year.append(e);
				}
			}

			// Prev year
			$('.control-left').live(
				'click',
				function() {
					var year = $(this).data('year');
					year = year - 1;
					Calendar.Reload(year);
				}
			);

			// Next year
			$('.control-right').live(
				'click',
				function() {
					var year = $(this).data('year');
					year = year + 1;
					Calendar.Reload(year);
				}
			);
		});

		</script>
