		<script src="http://assets.over-blog-kiwi.com/themes/13/js/socialite/socialite.min.js"></script>

		<script type="text/javascript">
			{% if Custom('comment') == 'Facebook' and isSingle %}
				// Facebook comments
				(function(d, s, id) {
					var js, fjs = d.getElementsByTagName(s)[0];
					if (d.getElementById(id)) return;
						js = d.createElement(s);
						js.id = id;
						js.src = "//connect.facebook.net/{{ Blog.Lang }}_{{ Blog.Lang|upper }}/all.js#xfbml=1&appId={{ Custom('facebook_appid')|default('284865384904712')}}";
						fjs.parentNode.insertBefore(js, fjs);
				}(document, 'script', 'facebook-jssdk'));
			{% elseif Custom('comment') == 'Disqus' and isSingle %}
				// Disqus comments
				var disqus_shortname = '{{ Custom('comment_disqus') }}';
				(function() {
					var dsq = document.createElement('script');
						dsq.type = 'text/javascript';
						dsq.async = true;
						dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
					(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
				})();
			{% endif %}


			<!-- Social networks : Twitter -->
			!function(d,s,id){
				var js, fjs = d.getElementsByTagName(s)[0];
				if(!d.getElementById(id)){
					js = d.createElement(s);
					js.id = id;
					js.src = "http://platform.twitter.com/widgets.js";
					fjs.parentNode.insertBefore(js, fjs);
				}
			}(document,"script","twitter-wjs");


			{% if Custom('google_analytics') is not empty %}
				<!-- Google Analytics -->
				window.google_analytics_uacct = '{{Custom('google_analytics')}}';
				var _gaq = _gaq || [];
					_gaq.push(['_setAccount', '{{Custom('google_analytics')}}']);
					_gaq.push(['_trackPageview']);
					_gaq.push(['_trackPageLoadTime']);

				(function() {
					var ga = document.createElement('script');
						ga.type = 'text/javascript';
						ga.async = true;
						ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
					var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
				})();
				<!-- End Google Analytics -->
			{% endif %}


			var Timeline = new Array();

			Timeline.elsStart = null,
			Timeline.counter = 1,
			Timeline.MAXPAGE = 10,

			// Set pictures height and width
			Timeline.EnhancePictures = function (elements) {
				$(elements).find('.ob-section-text img, .ob-section-images img').each(function(){
					$(this).attr(
						'src',
						$(this).attr('src').replace(/\/[0-9]+x[0-9]+(\-ct)?/, "/408x306-ct")
					);
				});
			},

			// Dispatch posts on columns
			Timeline.ItemDispatcher = function(elements)
			{
				if(elements){
					$(elements).hide();
					for (var i = 0, len = elements.length; i < len; i++)
					{
						if ($(".items.left").outerHeight(true) <= $(".items.right").outerHeight(true))
						{
							$(elements[i]).appendTo(".items.left");
						}
						else
						{
							$(elements[i]).appendTo(".items.right");
						}
						$(elements[i]).show();
					}
				}
			},

			// Set fancybox to all img except img in slideshows
			Timeline.Fancybox = function (elements) {
				// FancyBox
				var els = $(elements).find(".ob-section-images:not(.ob-layout900) a, .ob-section-text .ob-link-img").attr("rel", "fancybox");

				els.fancybox({
					'overlayShow'	: true,
					'overlayColor'  : '#000',
					'transitionIn'	: 'elastic',
					'transitionOut'	: 'elastic',
					'type'			: 'image'
				});
			},

			// Load sharing buttons on hover
			Timeline.Socialite = function (elements, hover) {
				if(hover){
					$(elements).find('.share').one('mouseenter', function(){
						Socialite.load($(this)[0]);
					});
				}else{
					Socialite.load($(elements));
				}
			},

			Timeline.Placeholder = function () {
				// Placeholder Script
				if($.browser.msie && $.browser.version <= 8.0 ){
					$('input[placeholder]').each(function(){
						var input = $(this);
						$(input).val(input.attr('placeholder'));
						$(input).focus(function(){
							 if (input.val() == input.attr('placeholder')) {
								 input.val('');
							 }
						});
						$(input).blur(function(){
							if (input.val() == '' || input.val() == input.attr('placeholder')) {
								input.val(input.attr('placeholder'));
							}
						});
					});
				}
			},

			Timeline.Search = function () {
				// Search form hack
				$('form.search').submit(function(e){
					e.preventDefault();
					window.location = $(this).attr('action') + $(this).find('input').attr('value');
					return false;
				});
			},

			Timeline.Strech = function () {
				if($.browser.msie && $.browser.version <= 8.0 ){
					{% if Custom('background_repeat') == 'strech' and Custom('background_image') is not empty %}
						// Backstretch
						$(document).ready(function(){
							//  Backstrech
							$.backstretch(
								'{{ Custom('background_image') }}'
							);
							$('.container').css('background', 'none');
						});
					{% endif %}
				}
			}

			$(document).ready(function(){
				{% if not isSingle %}
					Timeline.elsStart = $('.item');
					Timeline.EnhancePictures(Timeline.elsStart);
					$('.items-wrapper').imagesLoaded(function() {
						Timeline.ItemDispatcher(Timeline.elsStart);
					});

					Timeline.Fancybox(Timeline.elsStart);
					Timeline.Socialite(Timeline.elsStart, true);
					Timeline.Placeholder();
					Timeline.Search();
					Timeline.Strech();

					// loadmore plugin
					jQuery.ias({
						'container' 	: ".items-wrapper",
						'item' 			: ".item",
						'pagination' 	: ".load-more",
						'next'			: ".load-more a",
						'tresholdMargin': -250,
						'loader' 		: "<div class='loader'><img src='http://assets.over-blog-kiwi.com/themes/13/images/loader.gif' alt='loading' title='' /></div>",
						'history'		: false,
						beforePageChange: function(){

							if(Timeline.counter >= Timeline.MAXPAGE) {
								$('.load-more').show();
								return false;
							}
						},
						onRenderComplete: function(items){
							Timeline.EnhancePictures(items);

							if (typeof OB != "undefined") { OB.restart && OB.restart(); }
							if (typeof twttr != "undefined") { twttr.widgets.load(); }

							$('.items-wrapper').imagesLoaded(function() {
								Timeline.ItemDispatcher(items);
							});

							Timeline.Fancybox(items);
							Timeline.Socialite(items, true);
							Timeline.counter++;
						}
					});
				{% else %}
					Timeline.elsStart = $('.item');
					Timeline.Fancybox(Timeline.elsStart);
					Timeline.Socialite(Timeline.elsStart, false);
					Timeline.Placeholder();
					Timeline.Search();
					Timeline.Strech();
				{% endif %}
			});
		</script>
	</body>
</html>